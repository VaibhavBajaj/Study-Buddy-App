<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.illinois.finalproject">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".home.HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".auth.SignInActivity" />
        <activity android:name=".UserSessionManager" />
        <activity android:name=".helper.AddCourseActivity" />
        <activity android:name=".auth.SignUpActivity" />
        <activity android:name=".helper.AddMeetingActivity" />
        <activity android:name=".helper.AddBuddyActivity" />
        <activity android:name=".helper.BuddyDetailedActivity" />
        <activity android:name=".helper.CourseDetailedActivity"></activity>
    </application>

</manifest>